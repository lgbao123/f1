				}
			},
			{
				"name": "transform_constructor",
				"type": "DatabricksNotebook",
				"dependsOn": [
					{
						"activity": "transform_result_races",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"notebookPath": "/Repos/kreutergarrickus@outlook.com/f1/trans/constructor_standings",
					"baseParameters": {
						"p_file_date": {
							"value": "@formatDateTime(pipeline().parameters.p_end_date,'yyyy-MM-dd')",
							"type": "Expression"
						}
					}
				},
				"linkedServiceName": {
					"referenceName": "ls_f1_databricks",
					"type": "LinkedServiceReference"
				}
			},
			{
				"name": "Copy Result Races to sql",
				"type": "Copy",
				"dependsOn": [
					{
						"activity": "transform_result_races",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "ParquetSource",
						"storeSettings": {
							"type": "AzureBlobFSReadSettings",
							"recursive": true,
							"wildcardFolderPath": "race_results",
							"wildcardFileName": "*.parquet",
							"enablePartitionDiscovery": true,
							"partitionRootPath": "presentation/race_results"
						},
						"formatSettings": {
							"type": "ParquetReadSettings"
						}
					},
					"sink": {
						"type": "AzureSqlSink",
						"writeBehavior": "upsert",
						"upsertSettings": {
							"useTempDB": true,
							"keys": [
								"race_id",
								"driver_id",
								"team",
								"race_year"
							]
						},
						"sqlWriterUseTableLock": false,
						"disableMetricsCollection": false
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"typeConversion": true,
						"typeConversionSettings": {
							"allowDataTruncation": true,
							"treatBooleanAsNumber": false
						}
					}
				},
				"inputs": [
					{
						"referenceName": "ds_presentation_race_results",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "ds_sql_race_results",
						"type": "DatasetReference"
					}
				]
			},
