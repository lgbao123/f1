							"inputs": [
								{
									"referenceName": "rest_api_source_ds",
									"type": "DatasetReference",
									"parameters": {
										"relative_url": {
											"value": "@if( or(equals(toLower(item()['Load_Type']),'full'),\n        equals(item()['Is_First_Run'],'True'))\n    ,concat(\n        item()['Relative_Url'],\n        '?limit=1000&offset={offset}'\n    )\n    ,concat(\n        item()['Incremetal_Url'],\n        '?limit=1000&offset={offset}'\n    )\n)",
											"type": "Expression"
										}
									}
								}
							],
							"outputs": [
								{
									"referenceName": "raw_json_ds",
									"type": "DatasetReference",
									"parameters": {
										"source_system_name": {
											"value": "@item()['Source_System_Name']",
											"type": "Expression"
										},
										"source_object_name": {
											"value": "@item()['Source_Object_Name']",
											"type": "Expression"
										}
									}
								}
							]
						},
						{
							"name": "Get total",
							"type": "WebActivity",
							"dependsOn": [],
							"policy": {
								"timeout": "0.12:00:00",
								"retry": 0,
								"retryIntervalInSeconds": 30,
								"secureOutput": false,
								"secureInput": false
							},
							"userProperties": [],
							"typeProperties": {
								"method": "GET",
								"url": {
									"value": "@concat(\n    item()['Base_Url'],\n    item()['Relative_Url']\n)",
									"type": "Expression"
								}
							}
						},
						{
							"name": "Update is first run field",
							"type": "SqlServerStoredProcedure",
							"dependsOn": [
								{
									"activity": "Copy data from api to datalake",
									"dependencyConditions": [
										"Succeeded"
									]
								}
							],
							"policy": {
								"timeout": "0.12:00:00",
								"retry": 0,
								"retryIntervalInSeconds": 30,
								"secureOutput": false,
								"secureInput": false
							},
							"userProperties": [],
							"typeProperties": {
								"storedProcedureName": "[f1].[sp_UpdateIsFirstRun]",
								"storedProcedureParameters": {
									"Is_First_Run": {
										"value": "False",
										"type": "Boolean"
									},
									"Source_Object_Name": {
										"value": {
											"value": "@item()['Source_Object_Name']",
											"type": "Expression"
										},
										"type": "String"
									}
								}
							},
							"linkedServiceName": {
								"referenceName": "sqlserver2609_ls",
								"type": "LinkedServiceReference"
							}
						}
					]
				}
			},
			{
				"name": "Get control table",
				"type": "Lookup",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
