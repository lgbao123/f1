{
	"name": "ingest_full_incremental_load_pl",
	"properties": {
		"activities": [
			{
				"name": "ForEachTable",
				"type": "ForEach",
				"dependsOn": [
					{
						"activity": "Get control table",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"userProperties": [],
				"typeProperties": {
					"items": {
						"value": "@activity('Get control table').output.value",
						"type": "Expression"
					},
					"activities": [
						{
							"name": "Copy data from api to datalake",
							"type": "Copy",
							"dependsOn": [
								{
									"activity": "Get total",
									"dependencyConditions": [
										"Succeeded"
									]
								}
							],
							"policy": {
								"timeout": "0.12:00:00",
								"retry": 0,
								"retryIntervalInSeconds": 30,
								"secureOutput": false,
								"secureInput": false
							},
							"userProperties": [],
							"typeProperties": {
								"source": {
									"type": "RestSource",
									"httpRequestTimeout": "00:01:40",
									"requestInterval": "00.00:00:00.010",
									"requestMethod": "GET",
									"paginationRules": {
										"QueryParameters.{offset}": {
											"value": "@concat(\n'RANGE:0:',\nactivity('Get total').output.MRData.total,\n':1000')",
