{
	"name": "raw_ds",
	"properties": {
		"linkedServiceName": {
			"referenceName": "AzureDataLakeStorage_ls",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"source_system_name": {
				"type": "string"
			},
			"source_object_name": {
				"type": "string"
			},
			"target_object_name": {
				"type": "string"
			}
		},
		"annotations": [],
		"type": "Parquet",
		"typeProperties": {
			"location": {
				"type": "AzureBlobFSLocation",
				"fileName": {
					"value": "@dataset().target_object_name",
					"type": "Expression"
				},
				"folderPath": {
					"value": "@concat('raw',\r\n    '/',dataset().source_system_name,\r\n    '/',dataset().source_object_name,\r\n    '/year=',utcNow('yyyy'),\r\n    '/month=',utcNow('MM'),\r\n    '/day=',utcNow('dd')\r\n)",
					"type": "Expression"
				},
				"fileSystem": "fo1"
			},
			"compressionCodec": "snappy"
		},
		"schema": []
	},
	"type": "Microsoft.DataFactory/factories/datasets"
}